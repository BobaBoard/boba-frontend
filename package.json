  "main": "index.js",
  "scripts": {
    "v": "next -v",
    "dev": "cross-env DEBUG=bobafrontend:* next",
    "dev:stage": "cross-env NEXT_PUBLIC_DEFAULT_BACKEND=https://staging-dot-backend-dot-bobaboard.uc.r.appspot.com/ DEBUG=bobafrontend:* next",
    "dev:debug": "cross-env NODE_OPTIONS='--inspect' DEBUG=bobafrontend:* next dev",
    "build": "rm -rf .next && next build",
    "start": "next start -p 8080 ",
    "test": "jest",
    "test:watch": "jest --watch",
    "deploy": "rm -rf .next/ && rm -rf build/ && yarn run build && gcloud config set project bobaboard && gcloud app deploy --version live",
    "stage": "rm -rf .next/ && rm -rf build/ && yarn run build && gcloud config set project bobaboard && gcloud app deploy --quiet --version staging  && gcloud app services set-traffic frontend --splits live=1  --quiet"
  },
