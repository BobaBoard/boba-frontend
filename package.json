{
  "name": "bobaboard-frontend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "v": "next -v",
    "dev": "cross-env DEBUG=bobafrontend:*-log next",
    "dev:mock": "cross-env NEXT_PUBLIC_MSW=true DEBUG=bobafrontend:* next",
    "dev:stage": "cross-env NEXT_PUBLIC_DEFAULT_BACKEND=https://staging-dot-backend-dot-bobaboard.uc.r.appspot.com/ NEXT_PUBLIC_ENV=staging DEBUG=bobafrontend:* next",
    "dev:debug": "cross-env NODE_OPTIONS='--inspect' DEBUG=bobafrontend:* next dev",
    "build": "rm -rf .next && next build",
    "start": "next start -p 8080 ",
    "test": "jest",
    "test:watch": "jest --watch",
    "cypress": "cypress open",
    "cypress:headless": "cypress run --browser chrome --headless",
    "test:e2e:generate": "tsc -p e2e/tsconfig.json && mv e2e/dist/generate-examples.js e2e/dist/generate-examples.mjs && node --experimental-modules --loader ts-node/esm.mjs e2e/dist/generate-examples.mjs && prettier --write e2e/mocks/generated.ts",
    "test:e2e": "start-server-and-test 'yarn dev:mock' 3000 'yarn cypress'",
    "test:e2e:ci": "start-server-and-test 'yarn dev:mock' 3000 'yarn cypress:headless'",
    "deploy": "rm -rf .next/ && rm -rf build/ && yarn run build && gcloud config set project bobaboard && gcloud app deploy --version live",
    "stage": "rm -rf .next/ && rm -rf build/ && yarn run build && gcloud config set project bobaboard && gcloud app deploy --quiet --version staging --no-promote"
  },
  "author": "",
  "license": "ISC",
