  "main": "index.js",
  "scripts": {
    "build": "rm -rf .next && next build",
    "cypress": "cypress open",
    "cypress:headless": "cypress run --browser chrome --headless",
    "deploy": "rm -rf .next/ && rm -rf build/ && yarn run build && gcloud config set project bobaboard && gcloud app deploy --version live",
    "dev": "cross-env DEBUG=bobafrontend:*-log next",
    "dev:mgdns": "concurrently --kill-others \"node mgdns_proxies.js\" \"yarn run dev\"",
    "dev:bonjour": "concurrently --kill-others \"node bonjour_proxies.js\" \"yarn run dev\"",
    "dev:debug": "cross-env NODE_OPTIONS='--inspect' DEBUG=bobafrontend:* next dev",
    "dev:stage": "cross-env NEXT_PUBLIC_DEFAULT_BACKEND=https://staging-dot-backend-dot-bobaboard.uc.r.appspot.com/ NEXT_PUBLIC_ENV=staging DEBUG=bobafrontend:*-log next",
    "stage": "rm -rf .next/ && rm -rf build/ && yarn run build && gcloud config set project bobaboard && gcloud app deploy --quiet --version staging --no-promote",
    "start": "next start -p 8080 ",
    "test": "jest --roots tests --runInBand",
    "test:UI:watch": "yarn run test:watch tests/UI",
    "test:ci": "yarn run test --ci",
    "test:e2e": "start-server-and-test 'yarn dev:bonjour' http://twisted-minds_boba.local:3000/ 'yarn cypress'",
    "test:e2e:ci": "start-server-and-test 'yarn dev:bonjour' http://twisted-minds_boba.local:3000/ 'yarn cypress:headless'",
    "test:watch": "cross-env DEBUG=bobafrontend:*-log jest --watch"
  },
